		"typescript": "^5.0.0",
		"vite": "^5.0.3"
	},
	"dependencies": {
		"@fontsource/inter": "^5.1.0",
		"@fontsource/open-sans": "^5.1.0",
		"@prisma/client": "^5.22.0",
		"@sveltejs/adapter-node": "^5.2.9",
		"@types/express": "4",
		"discord.js": "^14.16.3",
		"express": "^4.21.1",
		"rollup-obfuscator": "^4.1.1",
		"zod": "^3.23.8"
